{
  "name": "@santi020k/eslint-config-react",
  "version": "0.0.0",
  "description": "React ESLint configuration with hooks and JSX rules",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@santi020k/eslint-config-core": "*",
    "@eslint/compat": "^2.0.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^8.54.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "santi020k",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santi020k/eslint-config.git",
    "directory": "packages/react"
  }
}
